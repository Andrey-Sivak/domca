{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/professional-background",
  "version": "0.1.0",
  "title": "Professional Background",
  "category": "widgets",
  "description": "A professional credentials section with headline, quote bubble, and experience details.",
  "keywords": [
    "professional",
    "experience",
    "credentials",
    "quote",
    "background"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "quoteIntro": {
      "type": "string",
      "default": ""
    },
    "quote": {
      "type": "string",
      "default": ""
    },
    "textOne": {
      "type": "string",
      "default": ""
    },
    "textTwo": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
