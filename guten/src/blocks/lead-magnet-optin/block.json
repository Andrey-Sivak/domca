{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/lead-magnet-optin",
  "version": "0.1.0",
  "title": "Lead Magnet Opt-in",
  "category": "widgets",
  "description": "A lead magnet section with speech bubble quote, bullet points, and download CTA button.",
  "keywords": [
    "lead magnet",
    "optin",
    "download",
    "speech bubble",
    "cta",
    "quote"
  ],
  "attributes": {
    "dataFormId": {
      "type": "string",
      "default": ""
    },
    "bulletPoints": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string",
            "default": ""
          }
        }
      }
    },
    "button": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
