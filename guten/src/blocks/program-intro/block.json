{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/program-intro",
  "version": "0.1.0",
  "title": "Program Introduction",
  "category": "widgets",
  "description": "Introduction section with illustration and program description featuring restart messaging.",
  "keywords": [
    "program",
    "introduction",
    "restart",
    "wellness",
    "illustration"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "image": {
      "type": "object",
      "default": {
        "id": null,
        "url": ""
      }
    },
    "text": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
