{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/about-slider",
  "version": "0.1.0",
  "title": "About Slider",
  "category": "widgets",
  "description": "About Slider Block",
  "attributes": {
    "name": {
      "type": "string",
      "default": ""
    },
    "credentials": {
      "type": "string",
      "default": ""
    },
    "location": {
      "type": "string",
      "default": ""
    },
    "email": {
      "type": "string",
      "default": ""
    },
    "instagram": {
      "type": "string",
      "default": ""
    },
    "images": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "image": {
            "type": "object",
            "default": {
              "id": 0,
              "url": ""
            }
          }
        }
      }
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
