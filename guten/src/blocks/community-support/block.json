{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/community-support",
  "version": "0.1.0",
  "title": "Community Support",
  "category": "widgets",
  "description": "A block to display community support message with illustration and key points",
  "keywords": [
    "community",
    "support",
    "women",
    "motivation",
    "illustration",
    "testimonial"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "subtitle": {
      "type": "string",
      "default": ""
    },
    "introText": {
      "type": "string",
      "default": ""
    },
    "motivationText": {
      "type": "string",
      "default": "They're showing up — for themselves.\nJUST LIKE YOU."
    },
    "honestTitle": {
      "type": "string",
      "default": ""
    },
    "honestPoints": {
      "type": "array",
      "default": [
        "We don't just want to feel better.",
        "We want to feel attractive.",
        "To look in the mirror — and see ourselves again.",
        "Alive. Confident. Light."
      ]
    },
    "closingText": {
      "type": "string",
      "default": ""
    },
    "button": {
      "type": "string",
      "default": ""
    },
    "image": {
      "type": "object",
      "default": {
        "id": null,
        "url": ""
      }
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
