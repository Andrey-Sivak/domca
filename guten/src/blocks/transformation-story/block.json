{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/transformation-story",
  "version": "0.1.0",
  "title": "Transformation Story",
  "category": "widgets",
  "description": "A personal transformation story section with headline, narrative text, and before/after photo gallery in masonry layout.",
  "keywords": [
    "transformation",
    "story",
    "before after",
    "gallery",
    "journey",
    "personal",
    "results"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "subtitle": {
      "type": "string",
      "default": ""
    },
    "images": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "image": {
            "type": "object",
            "default": {
              "id": null,
              "url": ""
            }
          }
        }
      }
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
