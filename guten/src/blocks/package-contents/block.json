{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/package-contents",
  "version": "0.1.0",
  "title": "Package Contents",
  "category": "widgets",
  "description": "A block to display package contents with flowing arrow design and feature details",
  "keywords": [
    "package",
    "contents",
    "features",
    "flow",
    "arrow",
    "benefits"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "feature1Title": {
      "type": "string",
      "default": ""
    },
    "feature1Details": {
      "type": "array",
      "default": []
    },
    "feature2Title": {
      "type": "string",
      "default": ""
    },
    "feature2Details": {
      "type": "array",
      "default": []
    },
    "feature3Title": {
      "type": "string",
      "default": ""
    },
    "feature3Details": {
      "type": "array",
      "default": []
    },
    "feature4Title": {
      "type": "string",
      "default": ""
    },
    "feature4Details": {
      "type": "array",
      "default": []
    },
    "bonusTitle": {
      "type": "string",
      "default": ""
    },
    "bonusDetails": {
      "type": "array",
      "default": []
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
