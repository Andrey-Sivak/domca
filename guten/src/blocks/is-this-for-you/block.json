{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/is-this-for-you",
  "version": "0.1.0",
  "title": "Is This For You",
  "category": "widgets",
  "description": "A qualification section with headline and multiple pain point/qualification cards in a grid layout.",
  "keywords": [
    "qualification",
    "target audience",
    "pain points",
    "cards",
    "grid"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "card1": {
      "type": "string",
      "default": ""
    },
    "card2": {
      "type": "string",
      "default": ""
    },
    "card3": {
      "type": "string",
      "default": ""
    },
    "card4": {
      "type": "string",
      "default": ""
    },
    "card5": {
      "type": "string",
      "default": ""
    },
    "card6": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
