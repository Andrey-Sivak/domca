{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/what-i-do-today",
  "version": "0.1.0",
  "title": "What I Do Today",
  "category": "widgets",
  "description": "Two-column section showcasing current activities and services with rounded cards.",
  "keywords": [
    "services",
    "activities",
    "today",
    "columns",
    "cards"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "text1": {
      "type": "string",
      "default": ""
    },
    "text2": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
