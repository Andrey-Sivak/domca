{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/three-cards-section",
  "version": "0.1.0",
  "title": "Three Cards Section",
  "category": "widgets",
  "description": "A reusable section with intro text, three feature cards, and concluding text. Perfect for benefits, features, or process steps.",
  "keywords": [
    "cards",
    "features",
    "benefits",
    "three",
    "section"
  ],
  "attributes": {
    "preTitle": {
      "type": "string",
      "default": "This is a real reset."
    },
    "title": {
      "type": "string",
      "default": "Created by a real woman â€” for real life."
    },
    "bottomText": {
      "type": "string",
      "default": ""
    },
    "card1": {
      "type": "string",
      "default": ""
    },
    "card2": {
      "type": "string",
      "default": ""
    },
    "card3": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
