{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/faq-about-section",
  "version": "0.1.0",
  "title": "FAQ & About Section",
  "category": "widgets",
  "description": "A two-column FAQ and About section with rounded cards and buttons.",
  "keywords": [
    "faq",
    "about",
    "cards"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "faqText": {
      "type": "string",
      "default": ""
    },
    "faqButton": {
      "type": "string",
      "default": ""
    },
    "aboutText": {
      "type": "string",
      "default": ""
    },
    "aboutButton": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
