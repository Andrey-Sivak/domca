{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/holiday-support",
  "version": "0.1.0",
  "title": "Holiday Support Section",
  "category": "widgets",
  "description": "A support section with illustration, headline, descriptive text and call-to-action for seasonal challenges.",
  "keywords": [
    "holiday",
    "support",
    "seasonal",
    "challenge",
    "illustration",
    "cta"
  ],
  "attributes": {
    "image": {
      "type": "object",
      "default": {
        "id": null,
        "url": ""
      }
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "text": {
      "type": "string",
      "default": ""
    },
    "button": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
