{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/dont-have-to-be-ready",
  "version": "0.1.0",
  "title": "Don't Have to Be Ready",
  "category": "widgets",
  "description": "A motivational section with empowering headline, supportive text, and call-to-action button.",
  "keywords": [
    "motivational",
    "empowerment",
    "cta",
    "ready",
    "willing"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "subtitle": {
      "type": "string",
      "default": ""
    },
    "text": {
      "type": "string",
      "default": ""
    },
    "emphasizedMessage": {
      "type": "string",
      "default": ""
    },
    "button": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
