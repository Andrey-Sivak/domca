{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/home-transformation-journey",
  "version": "0.1.0",
  "title": "4-Phase Transformation Journey",
  "category": "widgets",
  "description": "A testimonial block showcasing a 4-phase transformation journey with illustrated phases and customer success story.",
  "keywords": ["testimonial", "transformation", "journey", "phases", "success"],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "subtitle": {
      "type": "string",
      "default": ""
    },
    "items": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "image": {
            "type": "object",
            "default": {
              "id": null,
              "url": ""
            }
          },
          "title": {
            "type": "string",
            "default": ""
          },
          "text": {
            "type": "string",
            "default": ""
          }
        }
      }
    },
    "button": {
      "type": "string",
      "default": ""
    },
    "testimonialQuote": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
