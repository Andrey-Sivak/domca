{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "domca/personal-story",
  "version": "0.1.0",
  "title": "Personal Story Card",
  "category": "widgets",
  "description": "A rounded card block for displaying personal stories, testimonials, or reflections with elegant typography.",
  "keywords": [
    "story",
    "testimonial",
    "card",
    "personal",
    "quote"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "text": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "dm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
